      Procedure PAGE_MANAGEMENT_POPUP_STOCK_VARIATION(gnProductID is int)
      
      // Find the product
      HReadSeekFirst(product,ProductID,gnProductID)
      IF HFound(product) = False THEN
      	AddError("Request for managing the stocks of a non-existing product")
      	Error("Product not found")
      	PageCloseDialog(False)
      END
      
      // Is the stock managed for this product?
      IF product.LimitedQuantity = False THEN
      	AddError("Request for managing the stocks of a non-existing product")
      	Error("Stock not managed on this product")
      	PageCloseDialog(False)	
      END
      
      // Display the name of the product
      STC_NAME_PRODUCT = product.ProductCaption
      
      // Display the stock and the alert level
      //IF Product.StockAlertLevel = -1 then
      //	CBOX_MANAGE_ALERT_LEVEL[1] = False
      //ELSE
      //	CBOX_MANAGE_ALERT_LEVEL[1] = True
      //END
      //
      //EDT_Quantity = Product.Quantity
      //EDT_StockAlertLevel = Product.StockAlertLevel
      
      //Fill the looper of variations
      FillLPRVariations()
