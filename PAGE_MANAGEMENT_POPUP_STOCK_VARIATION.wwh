     code : |1-
      // Summary: Fills the looper of variations
      // Syntax:
      //FillLPRVariations ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      
      Procedure FillLPRVariations()
      
      nVariationID is int 
      sVariationCaption is string
      bDefaultVariation is boolean
      bIsDefaultColor	is boolean
      nImageID is int
      
      //First, clear the looper content
      LooperDeleteAll(LOOP_Variations)
      
      
      //Run the query used to retrieve the variations for the product passed in parameter
      QRY_ProductVariations.pProductID = gnProductID
      HExecuteQuery(QRY_ProductVariations)
      
      
      //The records are presented in the following format:
      //ProductID = 23	ProductVariationsID = 31	 	CharacteristicName= Color	CharacteristicValue = White
      //23	31	 	Size	M
      //23	32	 	Color	White
      //23	32	 	Size	L
      //23	33	 	Color	Petrol blue
      //23	33	 	Size	M
      //23	34	 	Color	Petrol blue
      //...
      
      FOR EACH QRY_ProductVariations 
      	
      	//If the variation ID was changed, add the previous variation, 
      	//and start from empty string for the caption of the variation 
      	//caption such as: Charact1: Value1, Charact2: Value2, Charact3: Value3...
      	IF nVariationID <> QRY_ProductVariations.ProductVariationID _AND_ sVariationCaption <> "" THEN
      		
      		IF bDefaultVariation = True THEN
      			LooperAddLine(LOOP_Variations, nVariationID, sVariationCaption,False,bDefaultVariation, nImageID,QRY_ProductVariations.IsColor)
      		ELSE
      			LooperAddLine(LOOP_Variations, nVariationID, sVariationCaption, True,bDefaultVariation, nImageID,QRY_ProductVariations.IsColor)
      		END
      		sVariationCaption = ""
      	END
      	
      	sVariationCaption += [", "] + QRY_ProductVariations.Name + [" : "] + QRY_ProductVariations.Value
      	bDefaultVariation = QRY_ProductVariations.ByDefault
      	nImageID = QRY_ProductVariations.ProductPhotoID
      	bIsDefaultColor = QRY_ProductVariations.IsColor
      	
      	//Stores the ID of the current variation	
      	nVariationID = QRY_ProductVariations.ProductVariationID
      	
      END
      
      IF sVariationCaption <> "" THEN
      	IF bDefaultVariation = True THEN
      		LooperAddLine(LOOP_Variations, nVariationID, sVariationCaption,False,bDefaultVariation, nImageID,bIsDefaultColor)
      	ELSE
      		LooperAddLine(LOOP_Variations, nVariationID, sVariationCaption, True,bDefaultVariation, nImageID,bIsDefaultColor)
      	END
      END
