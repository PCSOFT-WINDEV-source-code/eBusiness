     internal_properties : CAAAAAgAAAANK99aT578pqrY5vHKk6We4QlF1n4phI60i5kiJ2ajIXqY8ANw+K+Ev0qF5XyF7/X5nkR47wx/A9HQAdXUtV09qS9IbkPPF7nTWSaUTtcQidvXNTL45w==
     code : |1-
      _AddSiteHistory(CN_HISTORY_OPETYPE_CALL_WS,"Call to REST 'admin_rest'")
      
      LOCAL 
      	nProductID		is int
      	nPromotionID	is int
      	nFamilyID		is int
      
      // Do we have an operation number?
      SWITCH PageParameter(CN_ADMIN_REST_PARAM_IDOPERATION)
      	CASE ~=CN_ADMIN_REST_OPE_PRODUCT_GENERATION
      		NotifyInformation("Generating a product image: "+PageParameter(CN_ADMIN_REST_PARAM_PRODUCTID))
      		
      		// Ask for generating a product photo
      		nProductID = Val(PageParameter(CN_ADMIN_REST_PARAM_PRODUCTID))
      		
      		// Check the product
      		IF nProductID <=0 THEN
      			AddError("Product not recognized",PageParameter(CN_ADMIN_REST_PARAM_PRODUCTID))
      			StringDisplay("ERR")				
      		END
      		
      		// Start the generation
      		IF GenerateProductPhotos(nProductID) = False THEN
      			StringDisplay("ERR")
      		END
      
      	CASE ~= CN_ADMIN_REST_OPE_FAMILY_GENERATION
      		NotifyInformation("Generating an image of product family: "+PageParameter(CN_ADMIN_REST_PARAM_FAMILYID))
      		
      		// Ask for generating a family photo
      		nFamilyID = Val(PageParameter(CN_ADMIN_REST_PARAM_FAMILYID))
      		
      		// Check the family
      		IF nFamilyID <=0 THEN
      			AddError("Product not recognized",PageParameter(CN_ADMIN_REST_PARAM_FAMILYID))
      			StringDisplay("ERR")				
      		END
      		
      		// Start the generation		
      		IF GenerateFamilyPhoto(nFamilyID) = False THEN
      			StringDisplay("ERR")
      		END	
      	
      	CASE ~= CN_ADMIN_REST_OPE_PROMOTION_GENERATION
      		NotifyInformation("Generating a promotion image: "+PageParameter(CN_ADMIN_REST_PARAM_PROMOTIONID))
      		
      		// Ask for generating a promotion photo
      		nPromotionID = Val(PageParameter(CN_ADMIN_REST_PARAM_PROMOTIONID))
      		
      		// Check the promotion
      		IF nPromotionID <=0 THEN
      			AddError("Promotion not recognized",PageParameter(CN_ADMIN_REST_PARAM_PROMOTIONID))
      			StringDisplay("ERR")				
      		END
      		
      		// Start the generation
      		IF GeneratePromotionPhotos(nPromotionID) = False THEN
      			StringDisplay("ERR")
      		END	
      	
      	OTHER CASE
      		AddError("Operation not recognized",PageParameter(CN_ADMIN_REST_PARAM_IDOPERATION))
      		StringDisplay("ERR")	
      END
      
      // By default, everything is performed properly
      StringDisplay("")
      
      CASE EXCEPTION:
      	AddError("Exception not supported",ExceptionInfo())
      	StringDisplay("ERR")
