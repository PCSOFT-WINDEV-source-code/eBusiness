     code : |1-
      Procedure index()
      
      // Local variables
      LOCAL
      	sURLConfig is string
      
      // Was the site URL already stored?
      sURLConfig = ReadURLRestAdminPage()
      IF ReadURLRestAdminPage() = "" _OR_ Left(sURLConfig,Length("http://localhost"))~="http://localhost" THEN
      	// Store the URL of the site according to the URL of the index page
      	LOCAL
      		sURL is string = PageAddress("",paFULL)
      	
      	// Remove the parameters
      	IF Position(sURL,"?") > 0 THEN
      		sURL = sURL[[1 TO Position(sURL,"?")-1]]
      	END	
      		
      	// Replace the index.awp page by the REST management page
      	sURL = Replace(sURL,"index.awp",CS_ADMIN_REST_PAGENAME)
      	
      	// Store the URL
      	ModifyURLRestAdminPage(sURL)
      END
     type : 851968
   -
     code : |1+
      // According to the browser platform, the Web user can be redirected to the mobile version of the site
      IF BrowserPlatform() IN (BrowserPlatformRobot, BrowserPlatformStandard) THEN
      	
      	// Is there a promotion marked as main promotion?
      	HReadSeekFirst(Promotion, IsMain, True)
      	IF HFound(Promotion) THEN
      		
      		// Displays the page that displays the main promotion in "large"
      		PageDisplay(IndexMainPromotion)
      		
      	ELSE // No main promotion
      		
      		// Displays the requested index page
      		PageDisplay(ReadSiteHomePage())
      		
      	END
      	
      ELSE // Mobile platform
      	
      	// Displays the mobile home page
      	PageDisplay(home_mobile)
      	
      END
      
