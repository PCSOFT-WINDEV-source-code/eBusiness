  procedures :
   -
     name : Display
     procedure_id : 1416730325671700305
     type_code : 14
     code : |1+
      // Summary: Displays a promotion in banner
      // Syntax:
      //Display (<APromo> is STCOL_Promotion [, <bPurchaseVisible>])
      //
      // Parameters:
      //	APromo (STCOL_Promotion): Promotion to display
      //	bPurchaseVisible (default value=0): True to display the purchase links, False otherwise
      // Return value:
      // 	None
      //
      
      Procedure Display(APromo is STCOL_PROMOTION,bPurchaseVisible = False)
      
      
      IF APromo:LimitedQuantity = True THEN
      	STC_LimitedOffer = StringBuild("Offer limited to %1 products",APromo:Quantity)
      ELSE	
      	STC_LimitedOffer= ""
      END
      
      // Do we have to display the Buy link?
      IF bPurchaseVisible _AND_ APromo:ProductID > 0 THEN
      	EDT_ID = APromo:ProductID	
      ELSE
      	LINK_ClickHere..Visible = False
      	STC_BenefitPromo..Visible = False
      END
      
      // Display the image
      MAP_PROMOTION_BANNER = ReadPromotionPhoto(APromo.PromotionID,ROLE_PROMOTION_LARGE_LANDSCAPE)
     type : 458752
   -
     name : DisplayAccordingToID
     procedure_id : 1416730325671765841
     type_code : 14
     code : |1+
      // Summary: Displays a promotion in banner
      // Syntax:
      //DisplayAccordingToID (<nPromotionID> is int [, <bPurchaseVisible>])
      //
      // Parameters:
      //	nPromotionID (integer): Promotion to display
      //	bPurchaseVisible (default value=0): True to display the purchase links, False otherwise
      // Return value:
      // 	None
      //
      
      Procedure DisplayAccordingToID(nPromotionID is int,bPurchaseVisible = False)
      
      // Local variables
      stPromotion is STCOL_Promotion
      
      // Load the promotion
      stPromotion = ReadPromotion(nPromotionID)
      IF stPromotion.PromotionID = -1 THEN
      	// Hide the controls
      	LINK_ClickHere..Visible = False
      	STC_BenefitPromo..Visible = False
      	MAP_PROMOTION_BANNER..Visible = False
      END
      
      IF stPromotion:LimitedQuantity = True THEN
      	STC_LimitedOffer = StringBuild("Offer limited to %1 products",stPromotion:Quantity)
      ELSE	
      	STC_LimitedOffer= ""
      END
      
      // Stores the product identifier
      EDT_ID = stPromotion:ProductID	
      
      // Do we have to display the Buy link?
      IF bPurchaseVisible _AND_ stPromotion:ProductID > 0 THEN
      	EDT_ID = stPromotion:ProductID	
      ELSE
      	LINK_ClickHere..Visible = False
      	STC_BenefitPromo..Visible = False
      END
      
      // Display the image
      MAP_PROMOTION_BANNER = ReadPromotionPhoto(nPromotionID,ROLE_PROMOTION_LARGE_LANDSCAPE)
      
      // Hide the control if there is no promotion
      IF MAP_PROMOTION_BANNER = "" THEN 
      	MAP_PROMOTION_BANNER..Visible = False
      END
     type : 458752
