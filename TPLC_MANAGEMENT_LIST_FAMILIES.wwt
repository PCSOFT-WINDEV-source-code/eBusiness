     code : |1-
      // Summary: Fills the treeview of families
      // Syntax:
      //FillFamilyTreeview ([<sNode> [, <nParentIdentifier>]])
      //
      // This procedure is used to fill the treeview of families.
      // CAUTION: IT IS RECURSIVE
      // 
      // Parameters:
      //	sNode: Parent node into which the elements found must be added
      //	nParentIdentifier (default value=0): Identifier of the parent whose children must be found
      // Return value:
      // 	None
      //
      
      Procedure FillFamilyTreeview(sNode = <§$0000§>, nParentIdentifier = 0)
      
      // Local variables
      arrListFamilies 	is array of STCOL_Family
      stFamily			is STCOL_Family
      
      // Search for the families
      arrListFamilies = COL_Family.ListFamilies(nParentIdentifier)
      
      // Browse the families
      FOR ALL ELEMENT stFamily of arrListFamilies
      	// Add the family into the treeview
      	// with the node of the parent + TAB + caption of the family
      	TreeAdd(TREE_Families,sNode + TAB + stFamily.Caption,tvDefault,tvDefault,stFamily.FamilyID,tvLast)
      		
      	// Find the children of the family currently processed
      	FillFamilyTreeview(sNode + TAB + stFamily.Caption,stFamily.FamilyID)
      	
      END
     type : 458752
   -
     name : FillTreeview
     internal_properties : CAAAAAgAAABGrmJ/b1iiYK/stLVjDx/kPPmnpr1yisTwuUyz/h/Tda+V7QRdcRJp2izbU7vGn75HBviR7ZCMyd2yQhkCZ/TXFXiWYc/pIFy7QWSI5/1XfcuI6QpbdMz6IQVVx3qqnh/k4FHbPEW9alkzBbCL/ABP/M/lxHd9jI97teuxki3GwbJXsYA8yY3Jns9pffuQS3DNdPPKLnOVIefriiaX++xgg0zSgK4/Nl0uxHg7IPp2oibvLq3mmMNwC+B78tGoMLYeN9WXSg3Fz4Gr
     procedure_id : 1416730175125708325
     type_code : 14
     code : |1-
      // Summary: Fills the treeview of families
      // Syntax:
      // FillTreeview ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      
      Procedure FillTreeview()
      
       // Delete everything from the treeview
      TreeDeleteAll(TREE_Families)
      
      // Add the root of the family
      TreeAdd(TREE_Families,<§$0000§>,tvDefault,tvDefault,-1,tvFirst)
      
      // Fill the treeview of families
      FillFamilyTreeview()
      
      // Expands the entire treeview
      TreeExpandAll(TREE_Families)
      
      // Reload the entire data to be consistent with the treeview
      LoadFamilies(True, 0)
     type : 458752
